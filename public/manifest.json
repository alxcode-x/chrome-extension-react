{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
        "<all_urls>"
        ],
        "run_at": "document_idle",
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "16": "logo192.png",
      "32": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ]
}